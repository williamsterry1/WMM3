<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <LinearLayout
        android:layout_width="398dp"
        android:layout_height="199dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="2dp">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="match_parent"
            android:layout_height="125dp"
            app:srcCompat="@drawable/logo3" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="match_parent"
            android:layout_height="76dp"
            app:srcCompat="@drawable/progress" />

    </LinearLayout>

    <EditText
        android:id="@+id/editText"
        android:layout_width="@dimen/_230sdp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="First Name"
        android:textColor="#030303"
        android:textColorHint="#00000000"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="200dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="@dimen/_230sdp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Last Name"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="245dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#1B1919"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="306dp" />

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="240dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="471dp">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="209dp"
            android:layout_height="86dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="200dp"
            android:layout_marginTop="153dp"
            android:layout_marginEnd="2dp"
            android:layout_marginBottom="1dp"
            app:srcCompat="@drawable/signature" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="198dp"
            android:layout_height="match_parent"
            android:layout_alignEnd="@+id/imageView9"
            android:layout_marginEnd="212dp"
            app:srcCompat="@android:color/holo_red_dark" />

    </RelativeLayout>

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="209dp"
        android:layout_height="74dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/common_google_signin_btn_text_light_normal_background"
        tools:layout_editor_absoluteY="412dp" />
</android.support.constraint.ConstraintLayout>